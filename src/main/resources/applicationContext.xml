<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">


    <bean id="baseBallCoach"  class="com.tej.springdemo.BaseBallCoach">
        <constructor-arg ref="fortuneService"></constructor-arg>
    </bean>

    <bean id="trackCoach" class="com.tej.springdemo.TrackCoach">
        <property name="fortuneService" ref="fortuneService"></property>
        <property name="name" value="Baljit"></property>
        <property name="emailId" value="baljit.s@gmail.com"></property>

    </bean>
    <bean id="fortuneService"  class="com.tej.springdemo.FortuneServiceImpl">

    </bean>

</beans>